


<% filterStatusHelper = (collection, statusFillter, keyword) =>{
    let xhtmlResult = '';
    statusFillter.forEach((item) => {
        let link = systemConfig.prefixAdmin + '/'+ collection +'/' + item.value;
        if (keyword !== '' && keyword !== undefined) link += '?keyword=' + keyword;

        xhtmlResult += `<a class="mr-1 btn btn-sm btn-${item.class} btn-sm" href="/${link}"> ${item.name.toUpperCase() + "(" +  item.count + ")"} </a>`;    
    })
    return xhtmlResult;
} %>


