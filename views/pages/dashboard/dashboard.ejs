
<%
    let arrBox = [
        {name: 'Items', icon: 'ion ion-ios-people', link: `${systemConfig.prefixAdmin}/item-list`, total: countItems}
    ];
    let xtmlBox = '';
    arrBox.forEach(box =>{
        xtmlBox +=`<div class="col-lg-3 col-6">
                        <div class="small-box bg-info">
                            <div class="inner">
                                <h3>${box.total}</h3>
                                <p>${box.name}</p>
                            </div>
                            <div class="icon text-white">
                                <i class="${box.icon}"></i>
                            </div>
                            <a href="${box.link}" class="small-box-footer">More info <i class="fas fa-arrow-circle-right"></i></a>
                        </div>
                    </div>`
    })
%>

<%- xtmlBox %>
